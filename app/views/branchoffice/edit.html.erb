<div>
  <h1 class="font-bold text-4xl">Branchoffice#edit</h1>
  <%= form_with model: @branch_office do |form| %>
  <div class="row">
      <div class="card">
				<%= form.label :Nombre, :required => true %> <span class="color_form" ><%= form.text_field :name%></span><br>
				<%= form.label :Direccion, :required => true%> <span class="color_form"><%= form.text_field :direc%></span><br>
				<%= form.label :Telefono, :required => true %> <span class="color_form"><%= form.number_field :tel%></span><br>
				Localidad: <%= select_tag :location, options_from_collection_for_select(@locations, "id", "name"), {inlude_blanck: true} %>
			</div>
      <button class="btn btn-success"><%= form.submit "Crear Nueva Sucursal"%></button>
  </div>
  <% end%>
</div>
